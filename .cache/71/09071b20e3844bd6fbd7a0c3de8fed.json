l":{"line":26,"column":3}},{"generated":{"line":54,"column":2},"source":"js/CustomPass.js","original":{"line":28,"column":2},"name":"vertexShader"},{"generated":{"line":54,"column":14},"source":"js/CustomPass.js","original":{"line":28,"column":14}},{"generated":{"line":54,"column":16},"source":"js/CustomPass.js","original":{"line":28,"column":16}},{"generated":{"line":54,"column":183},"source":"js/CustomPass.js","original":{"line":37,"column":4}},{"generated":{"line":55,"column":2},"source":"js/CustomPass.js","original":{"line":39,"column":2},"name":"fragmentShader"},{"generated":{"line":55,"column":16},"source":"js/CustomPass.js","original":{"line":39,"column":16}},{"generated":{"line":55,"column":18},"source":"js/CustomPass.js","original":{"line":39,"column":18}},{"generated":{"line":56,"column":0},"source":"js/CustomPass.js","original":{"line":92,"column":0}},{"generated":{"line":56,"column":1},"source":"js/CustomPass.js","original":{"line":92,"column":1}}],"sources":{"js/CustomPass.js":"import { Vector2 } from \"three\";\n\n/** @module DotScreenShader */\n\n/**\n * Dot screen shader based on [glfx.js sepia shader]{@link https://github.com/evanw/glfx.js}.\n *\n * @constant\n * @type {ShaderMaterial~Shader}\n */\nconst CustomPass = {\n  name: \"DotScreenShader\",\n\n  uniforms: {\n    tDiffuse: { value: null },\n    time: { value: 0 },\n    scale: { value: 1 },\n    scale2: { value: 1 },\n    progress: { value: 0 },\n    tSize: { value: new Vector2(256, 256) },\n    center: { value: new Vector2(0.5, 0.5) },\n    angle: { value: 1.57 },\n    scale: { value: 1 },\n    uMouse: { value: new Vector2(0, 0) },\n    radius: { value: 0.2 },\n  },\n\n  vertexShader: /* glsl */ `\n\n\t\tvarying vec2 vUv;\n\n\t\tvoid main() {\n\n\t\t\tvUv = uv;\n\t\t\tgl_Position = projectionMatrix * modelViewMatrix * vec4( position, 1.0 );\n\n\t\t}`,\n\n  fragmentShader: /* glsl */ `\n\n\t\tuniform vec2 center;\n\t\tuniform float angle;\n    uniform float time;\n    uniform float scale;\n    uniform float scale2;\n    uniform float progress;\n\t\tuniform vec2 tSize;\n    uniform float radius;\n\n\t\tuniform sampler2D tDiffuse;\n    uniform sampler2D mTexture;\n\n\t\tvarying vec2 vUv;\n\n\t\tfloat pattern() {\n\n\t\t\tfloat s = sin( angle ), c = cos( angle );\n\n\t\t\tvec2 tex = vUv * tSize - center;\n\t\t\tvec2 point = vec2( c * tex.x - s * tex.y, s * tex.x + c * tex.y ) * scale;\n\n\t\t\treturn ( sin( point.x ) * sin( point.y ) ) * 4.0;\n\n\t\t}\n\n\t\tvoid main() {\n\n            vec2 uv = gl_FragCoord.xy / resolution.xy;\n            vec2 newUV = vUv;\n\n            //newUV = vUv + 0.1*vec2(sin(10. * vUv.x), sin(10.*vUv.y));\n\n            vec2 p = 1.*vUv - vec2(0.5);\n\n            p += 0.1*cos(scale*3.*p.yx + time + vec2(1.2, 3.4));\n            p += 0.1*cos(scale*3.7*p.yx + 1.4*time + vec2(2.2, 3.4));\n            p += 0.1*cos(scale*5.*p.yx + 2.6*time + vec2(4.2, 1.4));\n            p += 0.3*cos(scale*7.*p.yx + 2.6*time + vec2(10.2, 3.4));\n\n\t\t\t      //newUV = vUv + centeredUV*vec2(1.,0.);\n\n            newUV.x = mix(vUv.x, length(p), progress);\n            newUV.y = mix(vUv.y, 0., progress);\n            \n            vec4 color = texture2D( tDiffuse, newUV );\t\t\t\n            gl_FragColor = color;\n            \n\n\n    }\n\t\t`,\n};\n\nexport { CustomPass };\n"},"lineCount":null}},"error":null,"hash":"fffdb119554eca5ced5fc89b214ad2ee","cacheData":{"env":{}}}